<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:elevation="0dp">
    <FrameLayout android:background="@drawable/tabindicator" android:layout_width="match_parent" android:layout_height="wrap_content" android:elevation="0dp">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginHorizontal="10dp" android:layout_marginVertical="40dp">
            <LinearLayout android:layout_gravity="center" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="2">
                <TextView android:textAppearance="@style/TextAppearance.HomepagePreferenceTitle" android:textSize="26sp" android:ellipsize="marquee" android:id="@android:id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Manage Multi Users"/>
            </LinearLayout>
            <com.google.android.material.card.MaterialCardView android:layout_width="40dp" android:layout_height="40dp" app:cardBackgroundColor="@color/secondary_bg" app:cardCornerRadius="30dp" app:cardElevation="0dp" app:contentPadding="5dp" app:strokeColor="#00804d4d">
                <ImageView android:layout_gravity="center" android:id="@android:id/icon" android:layout_width="24dp" android:layout_height="24dp" android:src="@drawable/ic_manage_acc" android:contentDescription="@null" app:tint="@color/acc3"/>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>
    </FrameLayout>
</FrameLayout>
